# See prettier YAML format: https://prettier.io/docs/en/configuration.html
tabWidth: 4
printWidth: 120
singleQuote: true
trailingComma: "all"
bracketSpacing: true
overrides:
  - files: "*.sol"
    options:
      singleQuote: false
  - files: "*.yml"
    options:
      tabWidth: 2
      singleQuote: false
